{
  "name": "my-os",
  "version": "1.0.0",
  "description": "Windows operation system clone",
  "scripts": {
    "build": "npm run build:fs && next build",
    "build:fs": "cd public && node ../scripts/fs2json.js --exclude .index --out .index/fs.9p.json . && node ../node_modules/browserfs/dist/scripts/make_http_index.js > .index/fs.bfs.json",
    "build:robots": "node scripts/robots.js",
    "dev": "next dev",
    "eslint": "eslint .",
    "export": "next export",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "start": "next start",
    "stylelint": "stylelint **/*.tsx",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "prettier": "2.5.1",
    "stylelint": "^14.3.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js, ts, tsx}": "eslint --cache --fix",
    "*.tsx": "stylelint --fix"
  },
  "main": "jest.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnvarovM/my-os.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnvarovM/my-os/issues"
  },
  "homepage": "https://github.com/AnvarovM/my-os#readme"
}
